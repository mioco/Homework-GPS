        <Button Content="计算卫星位置" HorizontalAlignment="Left" Margin="50,91,0,0" VerticalAlignment="Top" Width="74" Click="positionOfGPS"/>
        <Button Content="基线长度" HorizontalAlignment="Left" Margin="522,91,0,0" VerticalAlignment="Top" Width="75" Click="lengthOfBaseline"/>
        <TextBox x:Name="filePath_Copy" HorizontalAlignment="Left" Height="37" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="584" Margin="0,37,0,0"/>
        <Button Content="浏览" HorizontalAlignment="Left" Margin="584,0,0,0" VerticalAlignment="Top" Width="77" Height="37" Click="readO"/>
        <Button Content="浏览" HorizontalAlignment="Left" Margin="584,37,0,0" VerticalAlignment="Top" Width="77" Height="37" Click="readN"/>
        <TextBox HorizontalAlignment="Left" Height="33" Margin="10,141,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="214"/>
        <TextBox HorizontalAlignment="Left" Height="33" Margin="247,141,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="187"/>
        <TextBox HorizontalAlignment="Left" Height="33" Margin="451,141,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="200"/>

    </Grid>
</Window>
